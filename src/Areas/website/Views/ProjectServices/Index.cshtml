@model IEnumerable<LIVESTOCK.Areas.website.Models.ProjectService>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/website/Views/Shared/_Layout.cshtml";
}


@section Styles {
    <!-- DataTables -->
    <link rel="stylesheet" href="~/libs/datatables/css/dataTables.bootstrap.min.css">

<style>
    td{        
    }
</style>
}

<div style="margin:20px;min-height:300px;font-family:Calibri">   
    <h3>Services provided to Farmer</h3>
        <table class="table table-striped table-bordered  dataTables-example">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.SerialNo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td class="text-center">
                        @Html.DisplayFor(modelItem => item.SerialNo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>
                </tr>
            }
        </tbody>            
    </table>    
</div>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Live Stock Balochistan</title>
    <link rel="icon" href="~/img/LDDDLogo2.png">
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/libs/bootstrap/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/libs/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/libs/ionicons/css/ionicons.min.css">
    <!-- Syncfusion Essential JS 2 Styles -->
    <link rel="stylesheet" href="https://cdn.syncfusion.com/ej2/18.2.46/material.css" />

    <!-- Syncfusion Essential JS 2 Scripts -->
    <script src="https://cdn.syncfusion.com/ej2/18.2.46/dist/ej2.min.js"></script>
    @RenderSection("Styles", required: false)    
    <!-- Theme style -->
    <link rel="stylesheet" href="~/libs/admin-lte/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/libs/admin-lte/css/skins/_all-skins.min.css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">

    <environment include="Development">
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>


</head>
<body class="hold-transition skin-green sidebar-mini fixed">


    <!-- ##### Header Area Start ##### -->
    <!-- Site wrapper -->
    <div class="wrapper">

        @await Html.PartialAsync("_Header")

        <!-- =============================================== -->
        <!-- Left side column. contains the sidebar -->
        @await Html.PartialAsync("_NavBar")

        <!-- =============================================== -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <div class="row" style="background-color:white">
                <div class="col-md-1 col-sm-2 col-lg-1 col-xs-2" style="padding:10px 0 0 40px">
                    <img src="~/img/LDDDLogo2.png" width="80" height="60" />
                </div>
                <div class="col-md-9  col-sm-7 col-xs-7">
                    <div class="text-center">
                        <h3>Livestock &amp; Dairy Development Department</h3>
                        <p class="display-4" style="font-size: 17px;font-weight:bold; color: #00A300">GOVERNMENT OF BALOCHISTAN</p>
                    </div>
                </div>
                <div class="col-md-1 col-sm-2 col-lg-1 col-xs-2" style="margin-top:15px"><img src="~/img/logo_gob.png" width="70" height="60" /></div>
            </div>
            @await Html.PartialAsync("_StatusMessage", TempData["StatusMessage"])

            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        <!-- =============================================== -->
        <!-- Page footer. -->
        @await Html.PartialAsync("_Footer")

        <!-- =============================================== -->
        <!-- Control Sidebar -->
        @await Html.PartialAsync("_SideBar")
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="~/libs/jquery/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/libs/bootstrap/js/bootstrap.min.js"></script>

    @RenderSection("Scripts", required: false)

    <!-- SlimScroll -->
    <script src="~/libs/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/libs/fastclick/fastclick.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/libs/admin-lte/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/libs/admin-lte/js/demo.js"></script>

    <!-- ASP.NET site script -->
    <environment include="Development">
        <script src="~/js/site.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    <script>
        $(document).ready(function () {
            $('.sidebar-menu').tree()
        })
    </script>

    @await Html.PartialAsync("_ValidationScriptsPartial")
    <ejs-scripts></ejs-scripts>
</body>
</html>